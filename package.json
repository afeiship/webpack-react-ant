{
  "name": "webpack-react-ant",
  "version": "1.0.0",
  "description": "Webpack for ant design.",
  "main": "index.js",
  "scripts": {
    "clean": "rimraf dist",
    "dll-dev": "webpack --config ./build/webpack.config.dll-dev.babel.js",
    "dll-prd": "webpack --config ./build/webpack.config.dll-prd.babel.js",
    "predev": "npm run clean && npm run dll-dev",
    "prebuild": "npm run clean && npm run dll-prd",
    "postbuild": "rimraf dist/vendors",
    "build": "webpack -p --env=prd",
    "dev": "webpack-dev-server --env=dev",
    "start": "node express.js"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/afeiship/webpack-react-ant.git"
  },
  "keywords": [
