    "react",
    "pc",
    "ant-design",
    "site",
    "website"
  ],
  "author": "afei",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/afeiship/webpack-react-ant/issues"
  },
  "homepage": "https://github.com/afeiship/webpack-react-ant#readme",
